[
  {
    "key": "cmd+'",
    "command": "workbench.action.toggleMaximizedPanel"
  },
  {
    "key": "cmd+k cmd+k",
    "command": "vscode-term.runCmd",
    "args": {
      "cmd": "EDITOR=code lf",
      "folder": "${projectFolder}"
    }
  },
  {
    "key": "cmd+k cmd+j",
    "command": "vscode-term.runCmd",
    "args": {
      "cmd": "xf --executable code",
      "folder": "${projectFolder}"
    }
  },
  {
    "key": "cmd+h",
    "command": "workbench.action.toggleActivityBarVisibility"
  }
]
