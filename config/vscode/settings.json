{
  "editor.suggestSelection": "first",
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "files.exclude": {
    "**/.classpath": true,
    "**/.project": true,
    "**/.settings": true,
    "**/.factorypath": true
  },
  "go.useLanguageServer": true,
  "vim.enableNeovim": true,
  "vim.easymotion": true,
  "vim.leader": "<space>",
  "vim.useSystemClipboard": true,
  "vim.hlsearch": true,
  "vim.incsearch": true,
  "vim.neovimPath": "/usr/local/bin/nvim",
  "vim.normalModeKeyBindings": [
    {
      "before": [";"],
      "after": [":"]
    },
    {
      "before": ["-"],
      "commands": [
        {
          "command": "vscode-cmdlet.runCmd",
          "args": {
            "cmd": "EDITOR=code lf",
            "folder": "${fileDirname}"
          }
        }
      ]
    },
    {
      "before": ["<leader>", "a", "a"],
      "commands": [
        {
          "command": "vscode-cmdlet.runCmd",
          "args": {
            "cmd": "xx \"${searchPhrase}\" --editor code",
            "folder": "${projectFolder}"
          }
        }
      ]
    },
    {
      "before": ["<leader>", "a", "s"],
      "commands": [
        {
          "command": "vscode-cmdlet.runCmd",
          "args": {
            "cmd": "xx ${wordUnderCursor} --editor code",
            "folder": "${projectFolder}"
          }
        }
      ]
    },
    {
      "before": ["<leader>", "d", "d"],
      "commands": [
        {
          "command": "vscode-cmdlet.runCmd",
          "args": {
            "cmd": "pwd",
            "folder": "${projectFolder}"
          }
        }
      ]
    },
    {
      "before": ["<leader>", "f"],
      "commands": [
        {
          "command": "vscode-cmdlet.runCmd",
          "args": {
            "cmd": "xf --executable code",
            "folder": "${projectFolder}"
          }
        }
      ]
    },
    {
      "before": ["<leader>", "h"],
      "commands": ["workbench.action.openRecent"]
    },
    {
      "before": ["<leader>", "o"],
      "commands": [
        {
          "command": "vscode-cmdlet.runCmd",
          "args": {
            "cmd": "nvim ${file}"
          }
        }
      ]
    },
    {
      "before": ["<leader>", "w"],
      "commands": [":w"]
    },
    {
      "before": ["<leader>", "v"],
      "commands": [":vsp"]
    },
    {
      "before": ["<leader>", "b"],
      "commands": ["editor.debug.action.toggleBreakpoint"]
    },
    {
      "before": ["<leader>", "x"],
      "commands": ["editor.action.formatDocument"]
    }
  ],
  "vim.easymotionMarkerBackgroundColor": "292c33",
  "vim.easymotionMarkerForegroundColorOneChar": "#d27272",
  "vim.easymotionMarkerForegroundColorTwoChar": "#dfc184",
  "editor.minimap.enabled": false,
  "breadcrumbs.enabled": true,
  "editor.renderWhitespace": "all",
  "editor.renderControlCharacters": true,
  "sync.gist": "d7de17ff8b015a674ba6e077f353e25c",
  "terminal.integrated.scrollback": 10000,
  "terminal.external.osxExec": "iTerm.app",
  "editor.fontFamily": "'Menlo for Powerline',Menlo, Monaco, 'Courier New', monospace",
  "editor.largeFileOptimizations": false,
  "workbench.iconTheme": "vscode-icons",
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "editor.lineNumbers": "on",
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "python.languageServer": "Pylance",
  "extensions.ignoreRecommendations": true,
  "git.ignoreMissingGitWarning": true,
  "workbench.editorAssociations": {
    "*.ipynb": "jupyter.notebook.ipynb"
  },
  "remote.SSH.useLocalServer": false,
  "remote.SSH.remotePlatform": {
    "yt": "linux"
  },
  "workbench.startupEditor": "none",
  "vim.camelCaseMotion.enable": true,
  "vim.visualstar": true,
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "terminal.integrated.defaultProfile.linux": "zsh",
  "terminal.integrated.copyOnSelection": true,
  "editor.tabSize": 8,
  "editor.cursorStyle": "line",
  "editor.insertSpaces": false,
  "editor.wordSeparators": "/\\()\"':,.;<>~!@#$%^&*|+=[]{}`?-",
  "editor.wordWrap": "off",
  "security.workspace.trust.untrustedFiles": "open"
}
