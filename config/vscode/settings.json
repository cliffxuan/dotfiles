{
  "go.useLanguageServer": true,
  "editor.suggestSelection": "first",
  "workbench.editorAssociations": {
    "*.ipynb": "jupyter.notebook.ipynb"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "editor.fontFamily": "'Menlo for Powerline',Menlo, Monaco, 'Courier New', monospace",
  "terminal.integrated.copyOnSelection": true,
  "terminal.integrated.scrollback": 10000,
  "vim.enableNeovim": true,
  "vim.visualstar": true,
  "vim.smartRelativeLine": true,
  "vim.hlsearch": true,
  "python.languageServer": "Pylance",
  "vim.easymotionMarkerForegroundColorOneChar": "#d27272",
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "vim.easymotionMarkerForegroundColorTwoChar": "#dfc184",
  "remote.SSH.useLocalServer": false,
  "[haskell]": {
    "editor.defaultFormatter": "haskell.haskell"
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "editor.renderControlCharacters": true,
  "editor.accessibilitySupport": "off",
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "vim.easymotion": true,
  "editor.wordSeparators": "/\\()\"':,.;<>~!@#$%^&*|+=[]{}`?-",
  "sync.gist": "d7de17ff8b015a674ba6e077f353e25c",
  "vscode-neovim.neovimExecutablePaths.darwin": "/usr/local/bin/nvim",
  "code-runner.executorMap": {
    "typescript": "node -r ts-node/register"
  },
  "editor.tabSize": 8,
  "editor.minimap.enabled": false,
  "workbench.iconTheme": "vscode-icons",
  "editor.wordWrap": "off",
  "git.ignoreMissingGitWarning": true,
  "vim.sneakReplacesF": true,
  "[yaml]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "security.workspace.trust.untrustedFiles": "open",
  "workbench.startupEditor": "none",
  "editor.largeFileOptimizations": false,
  "editor.cursorStyle": "line",
  "window.zoomLevel": 1,
  "code-runner.runInTerminal": true,
  "remote.SSH.remotePlatform": {
    "yt": "linux",
    "ab": "linux"
  },
  "extensions.ignoreRecommendations": true,
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "vim.leader": "<space>",
  "editor.insertSpaces": false,
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[rust]": {
    "editor.defaultFormatter": "rust-lang.rust-analyzer"
  },
  "vim.camelCaseMotion.enable": true,
  "terminal.external.osxExec": "iTerm.app",
  "editor.renderWhitespace": "all",
  "vim.neovimUseConfigFile": true,
  "vim.neovimPath": "/usr/local/bin/nvim",
  "vim.incsearch": true,
  "vim.normalModeKeyBindings": [
    {
      "commands": [
        {
          "command": "cmdlet.runCmd",
          "args": {
            "cmd": "EDITOR=code lf",
            "folder": "${fileDirname}"
          }
        }
      ],
      "before": [
        "-"
      ]
    },
    {
      "commands": [
        {
          "command": "cmdlet.runCmd",
          "args": {
            "cmd": "xx \"${searchPhrase}\" --editor code",
            "folder": "${projectFolder}"
          }
        }
      ],
      "before": [
        "<leader>",
        "a",
        "a"
      ]
    },
    {
      "commands": [
        {
          "command": "cmdlet.runCmd",
          "args": {
            "cmd": "xx ${wordUnderCursor} --editor code",
            "folder": "${projectFolder}"
          }
        }
      ],
      "before": [
        "<leader>",
        "a",
        "s"
      ]
    },
    {
      "commands": [
        {
          "command": "cmdlet.runCmd",
          "args": {
            "cmd": "pwd",
            "folder": "${projectFolder}"
          }
        }
      ],
      "before": [
        "<leader>",
        "d",
        "d"
      ]
    },
    {
      "commands": [
        {
          "command": "cmdlet.runCmd",
          "args": {
            "cmd": "xf --executable code",
            "folder": "${projectFolder}"
          }
        }
      ],
      "before": [
        "<leader>",
        "f"
      ]
    },
    {
      "commands": [
        "workbench.action.openRecent"
      ],
      "before": [
        "<leader>",
        "h"
      ]
    },
    {
      "commands": [
        {
          "command": "cmdlet.runCmd",
          "args": {
            "cmd": "pytest ${file} -k ${wordUnderCursor}"
          }
        }
      ],
      "before": [
        "<leader>",
        "k"
      ]
    },
    {
      "commands": [
        {
          "command": "cmdlet.runCmd",
          "args": {
            "cmd": "nvim ${file}"
          }
        }
      ],
      "before": [
        "<leader>",
        "o"
      ]
    },
    {
      "commands": [
        {
          "command": "cmdlet.runCmd",
          "args": {
            "cmd": "python ${file}"
          }
        }
      ],
      "before": [
        "<leader>",
        "r"
      ]
    },
    {
      "commands": [
        ":w"
      ],
      "before": [
        "<leader>",
        "w"
      ]
    },
    {
      "commands": [
        ":vsp"
      ],
      "before": [
        "<leader>",
        "v"
      ]
    },
    {
      "commands": [
        "editor.debug.action.toggleBreakpoint"
      ],
      "before": [
        "<leader>",
        "b"
      ]
    },
    {
      "commands": [
        "editor.action.formatDocument"
      ],
      "before": [
        "<leader>",
        "x"
      ]
    },
    {
      "commands": [
        {
          "command": "cmdlet.runCmd",
          "args": {
            "cmd": "pytest ${file}"
          }
        }
      ],
      "before": [
        "<leader>",
        "y"
      ]
    }
  ],
  "vim.useSystemClipboard": true,
  "files.exclude": {
    "**/.project": true,
    "**/.settings": true,
    "**/.classpath": true,
    "**/.factorypath": true
  },
  "terminal.integrated.defaultProfile.linux": "zsh",
  "redhat.telemetry.enabled": false,
  "vim.easymotionMarkerBackgroundColor": "292c33",
  "terminal.integrated.env.osx": {
    "FIG_NEW_SESSION": "1"
  },
  "python.formatting.provider": "black",
  "workbench.activityBar.visible": false
}
